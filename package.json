{
  "name": "wowjoy-component",
  "jest": {
    "verbose": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/es/",
      "<rootDir>/lib/"
    ],
    "transform": {
      "^.+\\.(j|t)sx?$": "babel7-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": false
  },
  "version": "3.5.24",
  "description": "wowjoy-component React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "create": "node scripts/createComponent.js",
    "ctest": "node scripts/createTest.js",
    "build": "npm run clean && npm run build:es && npm run build:lib",
    "build:es": "cross-env NODE_ENV=es npx babel src --out-dir es --copy-files --extensions \".js,.jsx,.ts,.tsx\"",
    "build:lib": "cross-env NODE_ENV=lib npx babel src --out-dir lib --copy-files --extensions \".js,.jsx,.ts,.tsx\"",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "restart": "npm run build && npm start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "@svgr/webpack": "^4.0.0",
    "react": "16.x",
    "react-dom": "16.x",
    "react-router-dom": "4.x",
    "styled-components": "^4.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@svgr/webpack": "^4.2.0",
    "@types/enzyme": "^3.9.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.2",
    "@types/react-test-renderer": "^16.8.1",
    "@types/styled-components": "^4.1.14",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel7-jest": "^1.0.1",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "cryptiles": "4.x",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "husky": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^9.0.2",
    "nwb": "^0.23.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.7.0",
    "source-map-loader": "^0.2.4",
    "styled-components": "4.x",
    "typescript": "^3.5.2",
    "typescript-plugin-styled-components": "^1.4.3",
    "url-parse": "1.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "manglobe",
  "homepage": "https://github.com/wow-joy/wowjoy-component",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ],
  "dependencies": {}
}
